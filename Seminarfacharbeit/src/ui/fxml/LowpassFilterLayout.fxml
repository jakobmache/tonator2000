<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<TitledPane animated="false" collapsible="false" text="Tiefpassfilter" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <HBox alignment="TOP_CENTER">
         <children>
            <VBox alignment="TOP_CENTER" spacing="5.0">
               <children>
                  <Label text="Cutoff" />
                  <Slider fx:id="cutoffSlider" max="2000.0" orientation="VERTICAL" showTickMarks="true" value="1000.0" />
                  <TextField fx:id="cutoffInput" onAction="#onCutoffInputAction" prefHeight="25.0" prefWidth="0.0" />
               </children>
            </VBox>
         </children>
      </HBox>
   </content>
</TitledPane>
