<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<VBox alignment="CENTER" onMouseClicked="#onMouseClicked" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
         <children>
            <VBox alignment="CENTER" spacing="5.0">
               <children>
                  <Label text="Cutoff" VBox.vgrow="NEVER" />
                  <Slider fx:id="cutoffSlider" max="1.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" min="0.01" orientation="VERTICAL" showTickMarks="true" value="1.0" VBox.vgrow="ALWAYS" />
                  <TextField fx:id="cutoffInput" editable="false" maxHeight="25.0" maxWidth="50.0" onAction="#onCutoffInputAction" prefHeight="25.0" prefWidth="50.0" VBox.vgrow="NEVER" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </VBox>
            <VBox alignment="TOP_CENTER" spacing="5.0">
               <children>
                  <Label text="Resonanz" />
                  <Slider fx:id="resonanceSlider" max="1.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" orientation="VERTICAL" showTickMarks="true" value="0.0" VBox.vgrow="ALWAYS" />
                  <TextField fx:id="resonanceInput" editable="false" maxHeight="25.0" maxWidth="50.0" onAction="#onResonanceInputAction" prefHeight="25.0" prefWidth="50.0" />
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </VBox>
         </children>
      </HBox>
   </children>
</VBox>
